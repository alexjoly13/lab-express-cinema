<h2>Ironhack Cinema</h2>

<p>Click on the movies to check the showtimes!</p>

<ul class="movie-list">
  {{#each movieArray}}
  <li>
    <img src="{{this.image}}" alt="">
    <p>{{this.title}}</p>

    <!-- embed the ID of the book into the address -->
    <a href="/movies/{{this._id}}">See more</a>
  </li>
  {{/each}}
</ul>